<!doctype html>
<html>
	<head>
		<title>LobbyPop! Admin</title>
		<link rel="stylesheet" href="/public/styles/smoothness/jquery-ui-1.10.0.css" />
		<link rel="stylesheet" href="/public/styles/common.css" />
		<link rel="stylesheet" href="/public/styles/admin.css" />
	</head>
	<body>
		<div class="page">
			<h1 class="brand"><span class="brand-text">LobbyPop!</span></h1>
			<div id="tabs">
				<ul>
					<li><a href="#displays">Displays</a></li>
					<li><a href="#sites">Sites</a></li>
					<li><a href="#actions">Actions</a></li>
				</ul>
				<div id="displays" align="center">
					<table>
						<tbody>
							<tr>
								<td id="display-a" width="50%">
									<a href="#displays" class="blank">
										<img src="/public/images/loading.gif" border="0" align="absmiddle" />
									</a>
								</td>
								<td id="display-b" width="50%">
									<a href="#displays" class="blank">
										<img src="/public/images/loading.gif" border="0" align="absmiddle" />
									</a>
								</td>
							</tr>
							<tr>
								<td id="display-c" width="50%">
									<a href="#displays" class="blank">
										<img src="/public/images/loading.gif" border="0" align="absmiddle" />
									</a>
								</td>
								<td id="display-d" width="50%">
									<a href="#displays" class="blank">
										<img src="/public/images/loading.gif" border="0" align="absmiddle" />
									</a>
								</td>
							</tr>
						</tbody>
					</table>
				</div>
				<div id="sites">
					<table>
						<thead>
							<tr>
								<th>Run</th>
								<th>Name</th>
								<th>URL</th>
								<th>Display</th>
								<th class="numeric">Time</th>
							</tr>
						</thead>
						<tbody></tbody>
					</table>
				</div>
			</div>
			<script type="text/javascript" src="/public/scripts/jquery-1.9.0.js"></script>
			<script type="text/javascript" src="/public/scripts/jquery-ui-1.10.0.js"></script>
			<script type="text/javascript" src="/public/scripts/admin.js"></script>
		</body>
	</div>
</html>
